<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="XamarinPrism.src._14_Aggregator.Views.RecievePage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <Editor
                HeightRequest="400"
                Placeholder="Enter message"
                Text="{Binding Message}" />
            <Button Command="{Binding OnSendEventCommand}" Text="Send Event" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>