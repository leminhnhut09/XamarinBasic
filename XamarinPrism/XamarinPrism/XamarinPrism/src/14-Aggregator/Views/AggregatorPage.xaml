<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="XamarinPrism.src._14_Aggregator.Views.AggregatorPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <Button Command="{Binding OnSubscribeCommand}" Text="Subscribe" />
            <Button Command="{Binding OnNavigationCommand}" Text="Go to view B" />
            <Button Command="{Binding OnUnsubscribeCommand}" Text="Unsubscribe" />
            <ListView ItemsSource="{Binding Collection}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>