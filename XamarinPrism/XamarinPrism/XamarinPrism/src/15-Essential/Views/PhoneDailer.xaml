<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="XamarinPrism.src._15_Essential.Views.PhoneDailer"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <Entry
                Keyboard="Telephone"
                Placeholder="Enter phonenumber"
                Text="{Binding PhoneNumber}" />
            <Button Command="{Binding OnClickCommand}" Text="Call" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>