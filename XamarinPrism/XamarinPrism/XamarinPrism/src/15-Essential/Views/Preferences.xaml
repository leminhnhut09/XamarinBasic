<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="XamarinPrism.src._15_Essential.Views.Preferences"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Remember" />
            <Switch IsToggled="{Binding IsToggle}" />
            <Button Command="{Binding OnSaveCommand}" Text="Remove" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>