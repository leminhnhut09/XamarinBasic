<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="XamarinPrism.src._15_Essential.Views.HapticFeedback"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <Button Command="{Binding OnClickCommand}" Text="Click" />
            <Button Command="{Binding OnLongPressCommand}" Text="Long Press" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>