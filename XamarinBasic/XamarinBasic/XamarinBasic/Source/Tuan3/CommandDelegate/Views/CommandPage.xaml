<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="XamarinBasic.Source.Tuan3.CommandDelegate.Views.CommandPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:XamarinBasic.Source.Tuan3.CommandDelegate.ViewModels">
    <!--<ContentPage.BindingContext>
        <vm:PushCommandViewModel />
    </ContentPage.BindingContext>-->
    <ContentPage.Content>
        <StackLayout>
            <Button
                Command="{Binding Command}"
                CommandParameter="2"
                Text="Send Number" />
            <Label Text="{Binding Text}" />
            <!--  CommandParameter="{Binding CommandParameter}"  -->
            <Button
                Command="{Binding NavigationCommand}"
                CommandParameter="2"
                Text="Send Number" />
            <Label x:Name="resultLabel" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>