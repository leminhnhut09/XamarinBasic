<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="XamarinBasic.Source.Tuan1.DetailLoginPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Detail Login">
    <ContentPage.Content>
        <StackLayout>
            <Label
                x:Name="userLabel"
                Margin="0,200"
                HorizontalOptions="CenterAndExpand"
                VerticalOptions="Center" />
            <Label
                x:Name="passLabel"
                HorizontalOptions="CenterAndExpand"
                VerticalOptions="Center" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>